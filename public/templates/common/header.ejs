                    <li class="nav-item">
                        <a class="nav-link" href="/add-hod"><i class="bi bi-person-add"></i><span>HOD</span></a>
                    </li>
                    <% } %>
                    <li class="nav-item">
                        <a class="nav-link collapsed" href="javascript:void(0)" data-toggle="collapse"
